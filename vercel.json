{
  "version": 2,
  "name": "sistema-votacao-semiologia",
  "builds": [
    {
      "src": "sistema-votacao-supabase.html",
      "use": "@vercel/static"
    }
  ],
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/sistema-votacao-supabase.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, must-revalidate"
        }
      ]
    }
  ]
}